<p id="notice"><%= notice %></p>

<h1>Listing Trenings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Exercise</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trenings.each do |trening| %>
      <tr>
        <td><%= trening.date %></td>
        <td><%= trening.exercise %></td>
        <td><%= trening.description %></td>
        <td><%= link_to 'Show', trening %></td>
        <td><%= link_to 'Edit', edit_trening_path(trening) %></td>
        <td><%= link_to 'Destroy', trening, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trening', new_trening_path %>
